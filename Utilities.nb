Cell[BoxData[
 RowBox[{
  RowBox[{"parenPositions", "[", "input_List", "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"posList", ",", "ds", ",", "char"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"posList", "=", 
      RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"ds", "=", 
      RowBox[{"CreateDataStructure", "[", "\"\<Stack\>\"", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Do", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"char", "=", 
         RowBox[{
         "input", "\[LeftDoubleBracket]", "pos", "\[RightDoubleBracket]"}]}], 
        ";", "\[IndentingNewLine]", 
        RowBox[{"Which", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"char", "\[Equal]", "\"\<(\>\""}], ",", 
          RowBox[{"ds", "[", 
           RowBox[{"\"\<Push\>\"", ",", 
            RowBox[{"{", 
             RowBox[{"\"\<(\>\"", ",", "pos"}], "}"}]}], "]"}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{"char", "\[Equal]", "\"\<)\>\""}], ",", 
          "\[IndentingNewLine]", "\t", 
          RowBox[{"If", "[", "\[IndentingNewLine]", "\t", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{"ds", "[", "\"\<Peek\>\"", "]"}], 
              "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
             "\[Equal]", "\"\<(\>\""}], ",", "\[IndentingNewLine]", "\t", 
            RowBox[{
             RowBox[{"AppendTo", "[", 
              RowBox[{"posList", ",", 
               RowBox[{"{", 
                RowBox[{
                 RowBox[{
                  RowBox[{"ds", "[", "\"\<Peek\>\"", "]"}], 
                  "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], ",",
                  "pos"}], "}"}]}], "]"}], ";", 
             RowBox[{"ds", "[", "\"\<Pop\>\"", "]"}]}], ",", 
            "\[IndentingNewLine]", "\t", 
            RowBox[{"ds", "[", 
             RowBox[{"\"\<Push\>\"", ",", 
              RowBox[{"{", 
               RowBox[{"\"\<)\>\"", ",", "pos"}], "}"}]}], "]"}]}], "]"}]}], 
         "\[IndentingNewLine]", "]"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{"pos", ",", 
         RowBox[{"Length", "[", "input", "]"}]}], "}"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Sort", "[", "posList", "]"}]}]}], "\[IndentingNewLine]", 
