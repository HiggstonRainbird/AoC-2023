                    RowBox[{
                    RowBox[{"i", "+", "1"}], "<=", 
                    RowBox[{"Length", "[", "viewBoard", "]"}]}], "\[And]", 
                    RowBox[{"StringMatchQ", "[", 
                    RowBox[{
                    RowBox[{"viewBoard", "\[LeftDoubleBracket]", 
                    RowBox[{
                    RowBox[{"i", "+", "1"}], ",", "j"}], 
                    "\[RightDoubleBracket]"}], ",", 
                    RowBox[{"Except", "[", 
                    RowBox[{"{", "\"\<#\>\"", "}"}], "]"}]}], "]"}]}], ",", 
