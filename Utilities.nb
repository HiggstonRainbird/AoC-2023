             RowBox[{
             "#", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], ",", 
             RowBox[{
             "#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}], 
            "\[RightDoubleBracket]"}], "\[Rule]", "#"}], "&"}], "/@", 
         RowBox[{"Position", "[", 
          RowBox[{"inputText", ",", 
           RowBox[{"_", "?", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"UpperCaseQ", "[", "#", "]"}], "&"}], ")"}]}], ",", 
           RowBox[{"{", "2", "}"}], ",", 
           RowBox[{"Heads", "\[Rule]", "False"}]}], "]"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"keys", "=", 
       RowBox[{"Join", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"\"\<@\>\"", "\[Rule]", "position"}], "}"}], ",", 
         RowBox[{"Sort", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"viewBoard", "\[LeftDoubleBracket]", 
              RowBox[{
               RowBox[{
               "#", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
               ",", 
               RowBox[{
               "#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}], 
              "\[RightDoubleBracket]"}], "\[Rule]", "#"}], "&"}], "/@", 
           RowBox[{"Position", "[", 
            RowBox[{"inputText", ",", 
             RowBox[{"_", "?", 
              RowBox[{"(", 
               RowBox[{
                RowBox[{"LowerCaseQ", "[", "#", "]"}], "&"}], ")"}]}], ",", 
             RowBox[{"{", "2", "}"}], ",", 
             RowBox[{"Heads", "\[Rule]", "False"}]}], "]"}]}], "]"}]}], 
        "]"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"style", "=", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"Background", "\[Rule]", 
          RowBox[{"GrayLevel", "[", "0", "]"}]}], ",", 
         RowBox[{"BaseStyle", "\[Rule]", 
          RowBox[{"{", 
           RowBox[{"Directive", "[", 
            RowBox[{"White", ",", 
             RowBox[{"EdgeForm", "[", "]"}], ",", 
             RowBox[{"Opacity", "[", "1", "]"}]}], "]"}], "}"}]}], ",", 
         RowBox[{"VertexShapeFunction", "\[Rule]", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"Rectangle", "[", 
             RowBox[{
              RowBox[{"#1", "+", ".5"}], ",", 
              RowBox[{"#1", "-", ".5"}]}], "]"}], "&"}], ")"}]}], ",", 
         RowBox[{"EdgeShapeFunction", "\[Rule]", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"Rectangle", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"#1", "[", 
                RowBox[{"[", "1", "]"}], "]"}], "+", ".5"}], ",", 
              RowBox[{
               RowBox[{"#1", "[", 
                RowBox[{"[", "2", "]"}], "]"}], "-", ".5"}]}], "]"}], "&"}], 
